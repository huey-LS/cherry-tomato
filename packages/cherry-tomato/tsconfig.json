{
  "compilerOptions": {
    "target": "es2015",
    "module": "CommonJS",
    "strict": true,
    "declaration": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "sourceMap": false,
    "lib": [
      "ESNext",
      "DOM"
    ],
  }
}
